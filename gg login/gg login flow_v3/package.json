{
  "name": "flow-login-v3",
  "version": "3.0.0",
  "description": "Google Flow Login Checker with GUI",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . \"Flow Login V3\" --platform=win32 --arch=x64 --out=dist --overwrite --ignore=node_modules/puppeteer/.local-chromium",
    "dev": "electron ."
  },
  "build": {
    "appId": "com.flowlogin.v3",
    "productName": "Flow Login V3",
    "forceCodeSigning": false,
    "win": {
      "target": "dir",
      "icon": "icon.ico",
      "signAndEditExecutable": false
    },
    "files": [
      "**/*",
      "!node_modules/puppeteer/.local-chromium/**/*"
    ],
    "asar": false
  },
  "dependencies": {
    "puppeteer": "^21.6.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-builder": "^26.4.0",
    "electron-packager": "^17.1.2"
  },
  "author": "Flow Login",
  "license": "MIT"
}